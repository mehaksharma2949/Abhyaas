{
  "name": "@devvai/devv-tagger-plugin",
  "version": "1.0.0",
  "description": "Vite/Webpack plugin to add source location attributes to JSX elements",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --outDir dist",
    "prepublishOnly": "npm run build",
    "deploy:test": "npm version prerelease --preid=dev && npm publish --tag dev",
    "deploy:prod": "npm version patch && npm publish"
  },
  "keywords": [
    "vite",
    "webpack",
    "plugin",
    "jsx",
    "devtools"
  ],
  "author": "",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "vite": ">=3.0.0 || >=4.0.0 || >=5.0.0"
  },
  "dependencies": {
    "@babel/parser": "^7.28.4",
    "estree-walker": "^3.0.3",
    "magic-string": "^0.30.19"
  },
  "devDependencies": {
    "@babel/traverse": "^7.28.4",
    "@types/node": "^24.5.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  }
}
